{
  "version": 2,
  "builds": [
      {
          "src": "main.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/api/(.*)",
          "dest": "main.js"
      },
      {
          "src": "/home",
          "dest": "/home/index.html"
      },
      {
          "src": "/home/(.*)",
          "dest": "/home/index.html"
      },
      {
          "src": "/(.*)",
          "dest": "/home/index.html"
      }
  ]
}
